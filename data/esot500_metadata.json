          "@type": "cr:Field",
          "@id": "default/seq_id",
          "name": "default/seq_id",
          "description": "Column 'seq_id' from the Hugging Face parquet file.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "seq_id"
            }
          }
        }
      ]
    }
